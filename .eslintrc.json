{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "ymaps": "readonly",
    "module": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-empty": ["error", { "allowEmptyCatch": true }]
  }
}
